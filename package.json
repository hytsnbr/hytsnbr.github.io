{
  "name": "hytsnbr.github.io",
  "private": true,
  "version": "0.0.0",
  "packageManager": "pnpm@9.7.0",
  "scripts": {
    "preinstall": "husky install",
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "pnpm build && vite preview",
    "lint:script": "eslint --fix src/*.{ts,vue} && eslint --fix src/**/*.{ts,vue}",
    "lint:style": "stylelint src/**/*.{css,scss,vue}",
    "lint": "pnpm lint:script && pnpm lint:style",
    "format": "prettier -w src/*.{ts,vue} && prettier -w src/**/*.{ts,vue}",
    "fix": "pnpm format && pnpm lint"
  },
  "dependencies": {
    "vue": "^3.4.37",
    "vue-router": "^4.4.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.4.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@types/node": "^22.1.0",
    "@typescript-eslint/eslint-plugin": "^8.0.1",
    "@typescript-eslint/parser": "^8.0.1",
    "@vitejs/plugin-vue": "^5.1.2",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "autoprefixer": "^10.4.20",
    "css-declaration-sorter": "^7.2.0",
    "daisyui": "^4.12.10",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-tailwindcss": "^3.17.4",
    "eslint-plugin-vue": "^9.27.0",
    "husky": "^9.1.4",
    "lint-staged": "^15.2.8",
    "postcss": "^8.4.41",
    "postcss-html": "^1.7.0",
    "postcss-sort-media-queries": "^5.2.0",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.5",
    "remixicon": "^4.3.0",
    "sass": "^1.77.8",
    "stylelint": "^16.8.1",
    "stylelint-config-recommended-scss": "^14.1.0",
    "stylelint-config-recommended-vue": "^1.5.0",
    "stylelint-config-standard": "^36.0.1",
    "tailwindcss": "^3.4.9",
    "typescript": "^5.5.4",
    "vite": "^5.4.0",
    "vue-tsc": "^2.0.29"
  },
  "lint-staged": {
    "*.{ts,vue}": "pnpm lint:script",
    "*.{css,scss,vue}": "pnpm lint:style"
  }
}
