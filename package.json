{
  "name": "hytsnbr.github.io",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "packageManager": "pnpm@10.30.3",
  "scripts": {
    "preinstall": "husky",
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "pnpm build && vite preview",
    "lint:script": "eslint --fix src/*.{ts,vue} && eslint --fix src/**/*.{ts,vue}",
    "lint:style": "stylelint src/**/*.{css,scss,vue}",
    "lint": "pnpm lint:script && pnpm lint:style",
    "format": "prettier -w src/*.{ts,vue} && prettier -w src/**/*.{ts,vue}",
    "fix": "pnpm format && pnpm lint"
  },
  "dependencies": {
    "vue": "3.5.29",
    "vue-router": "5.0.3"
  },
  "devDependencies": {
    "@commitlint/cli": "20.4.2",
    "@commitlint/config-conventional": "20.4.2",
    "@eslint/eslintrc": "3.3.4",
    "@eslint/js": "9.39.3",
    "@types/node": "25.3.2",
    "@typescript-eslint/eslint-plugin": "8.56.1",
    "@typescript-eslint/parser": "8.56.1",
    "@vitejs/plugin-vue": "6.0.4",
    "@vue/eslint-config-prettier": "10.2.0",
    "@vue/eslint-config-typescript": "13.0.0",
    "autoprefixer": "10.4.27",
    "css-declaration-sorter": "7.3.1",
    "daisyui": "5.5.19",
    "eslint": "9.39.3",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-tailwindcss": "3.18.2",
    "eslint-plugin-vue": "9.33.0",
    "globals": "17.3.0",
    "husky": "9.1.7",
    "lint-staged": "16.2.7",
    "postcss": "8.5.6",
    "postcss-html": "1.8.1",
    "postcss-sort-media-queries": "5.2.0",
    "prettier": "3.8.1",
    "prettier-plugin-tailwindcss": "0.7.2",
    "remixicon": "4.9.1",
    "sass": "1.97.3",
    "stylelint": "17.4.0",
    "stylelint-config-recommended-scss": "17.0.0",
    "stylelint-config-recommended-vue": "1.6.1",
    "stylelint-config-standard": "40.0.0",
    "tailwindcss": "3.4.19",
    "typescript": "5.9.3",
    "vite": "7.3.1",
    "vue-tsc": "3.2.5"
  },
  "lint-staged": {
    "*.{ts,vue}": "pnpm lint:script",
    "*.{css,scss,vue}": "pnpm lint:style"
  }
}
