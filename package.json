{
  "name": "hytsnbr.github.io",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "packageManager": "pnpm@10.20.0",
  "scripts": {
    "preinstall": "husky",
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "pnpm build && vite preview",
    "lint:script": "eslint --fix src/*.{ts,vue} && eslint --fix src/**/*.{ts,vue}",
    "lint:style": "stylelint src/**/*.{css,scss,vue}",
    "lint": "pnpm lint:script && pnpm lint:style",
    "format": "prettier -w src/*.{ts,vue} && prettier -w src/**/*.{ts,vue}",
    "fix": "pnpm format && pnpm lint"
  },
  "dependencies": {
    "vue": "3.5.22",
    "vue-router": "4.6.3"
  },
  "devDependencies": {
    "@commitlint/cli": "20.1.0",
    "@commitlint/config-conventional": "20.0.0",
    "@eslint/eslintrc": "3.3.1",
    "@eslint/js": "9.39.1",
    "@types/node": "24.10.0",
    "@typescript-eslint/eslint-plugin": "8.46.3",
    "@typescript-eslint/parser": "8.46.3",
    "@vitejs/plugin-vue": "6.0.1",
    "@vue/eslint-config-prettier": "10.2.0",
    "@vue/eslint-config-typescript": "13.0.0",
    "autoprefixer": "10.4.21",
    "css-declaration-sorter": "7.3.0",
    "daisyui": "5.4.3",
    "eslint": "9.39.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-tailwindcss": "3.18.2",
    "eslint-plugin-vue": "9.33.0",
    "globals": "16.5.0",
    "husky": "9.1.7",
    "lint-staged": "16.2.6",
    "postcss": "8.5.6",
    "postcss-html": "1.8.0",
    "postcss-sort-media-queries": "5.2.0",
    "prettier": "3.6.2",
    "prettier-plugin-tailwindcss": "0.7.1",
    "remixicon": "4.7.0",
    "sass": "1.93.3",
    "stylelint": "16.25.0",
    "stylelint-config-recommended-scss": "16.0.2",
    "stylelint-config-recommended-vue": "1.6.1",
    "stylelint-config-standard": "39.0.1",
    "tailwindcss": "3.4.18",
    "typescript": "5.9.3",
    "vite": "7.1.12",
    "vue-tsc": "3.1.3"
  },
  "lint-staged": {
    "*.{ts,vue}": "pnpm lint:script",
    "*.{css,scss,vue}": "pnpm lint:style"
  }
}
