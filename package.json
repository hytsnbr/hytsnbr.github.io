{
  "name": "hytsnbr.github.io",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "packageManager": "pnpm@9.13.2",
  "scripts": {
    "preinstall": "husky install",
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "pnpm build && vite preview",
    "lint:script": "eslint --fix src/*.{ts,vue} && eslint --fix src/**/*.{ts,vue}",
    "lint:style": "stylelint src/**/*.{css,scss,vue}",
    "lint": "pnpm lint:script && pnpm lint:style",
    "format": "prettier -w src/*.{ts,vue} && prettier -w src/**/*.{ts,vue}",
    "fix": "pnpm format && pnpm lint"
  },
  "dependencies": {
    "vue": "3.5.13",
    "vue-router": "4.4.5"
  },
  "devDependencies": {
    "@commitlint/cli": "19.5.0",
    "@commitlint/config-conventional": "19.5.0",
    "@eslint/eslintrc": "3.1.0",
    "@eslint/js": "9.15.0",
    "@types/node": "22.7.9",
    "@typescript-eslint/eslint-plugin": "8.14.0",
    "@typescript-eslint/parser": "8.8.0",
    "@vitejs/plugin-vue": "5.1.4",
    "@vue/eslint-config-prettier": "10.1.0",
    "@vue/eslint-config-typescript": "13.0.0",
    "autoprefixer": "10.4.20",
    "css-declaration-sorter": "7.2.0",
    "daisyui": "4.12.14",
    "eslint": "9.13.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-tailwindcss": "3.17.5",
    "eslint-plugin-vue": "9.29.1",
    "globals": "15.12.0",
    "husky": "9.1.6",
    "lint-staged": "15.2.10",
    "postcss": "8.4.49",
    "postcss-html": "1.7.0",
    "postcss-sort-media-queries": "5.2.0",
    "prettier": "3.3.3",
    "prettier-plugin-tailwindcss": "0.6.8",
    "remixicon": "4.5.0",
    "sass": "1.81.0",
    "stylelint": "16.10.0",
    "stylelint-config-recommended-scss": "14.1.0",
    "stylelint-config-recommended-vue": "1.5.0",
    "stylelint-config-standard": "36.0.1",
    "tailwindcss": "3.4.15",
    "typescript": "5.6.3",
    "vite": "5.4.11",
    "vue-tsc": "2.1.10"
  },
  "lint-staged": {
    "*.{ts,vue}": "pnpm lint:script",
    "*.{css,scss,vue}": "pnpm lint:style"
  }
}
