{
  "name": "hytsnbr.github.io",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "packageManager": "pnpm@9.15.9",
  "scripts": {
    "preinstall": "husky",
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "pnpm build && vite preview",
    "lint:script": "eslint --fix src/*.{ts,vue} && eslint --fix src/**/*.{ts,vue}",
    "lint:style": "stylelint src/**/*.{css,scss,vue}",
    "lint": "pnpm lint:script && pnpm lint:style",
    "format": "prettier -w src/*.{ts,vue} && prettier -w src/**/*.{ts,vue}",
    "fix": "pnpm format && pnpm lint"
  },
  "dependencies": {
    "vue": "3.5.13",
    "vue-router": "4.5.0"
  },
  "devDependencies": {
    "@commitlint/cli": "19.8.0",
    "@commitlint/config-conventional": "19.8.0",
    "@eslint/eslintrc": "3.3.1",
    "@eslint/js": "9.25.1",
    "@types/node": "22.14.1",
    "@typescript-eslint/eslint-plugin": "8.31.0",
    "@typescript-eslint/parser": "8.31.0",
    "@vitejs/plugin-vue": "5.2.3",
    "@vue/eslint-config-prettier": "10.2.0",
    "@vue/eslint-config-typescript": "13.0.0",
    "autoprefixer": "10.4.21",
    "css-declaration-sorter": "7.2.0",
    "daisyui": "5.0.28",
    "eslint": "9.25.1",
    "eslint-config-prettier": "10.1.2",
    "eslint-plugin-tailwindcss": "3.18.0",
    "eslint-plugin-vue": "9.33.0",
    "globals": "16.0.0",
    "husky": "9.1.7",
    "lint-staged": "15.5.1",
    "postcss": "8.5.3",
    "postcss-html": "1.8.0",
    "postcss-sort-media-queries": "5.2.0",
    "prettier": "3.5.3",
    "prettier-plugin-tailwindcss": "0.6.11",
    "remixicon": "4.6.0",
    "sass": "1.87.0",
    "stylelint": "16.18.0",
    "stylelint-config-recommended-scss": "14.1.0",
    "stylelint-config-recommended-vue": "1.6.0",
    "stylelint-config-standard": "38.0.0",
    "tailwindcss": "3.4.17",
    "typescript": "5.8.3",
    "vite": "6.3.2",
    "vue-tsc": "2.2.10"
  },
  "lint-staged": {
    "*.{ts,vue}": "pnpm lint:script",
    "*.{css,scss,vue}": "pnpm lint:style"
  }
}
